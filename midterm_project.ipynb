    "def new_date(date):\n",
    "    if len(date)==4:\n",
    "        return date.replace(date, date + '-01-01')\n",
    "    elif len(date)==7:\n",
    "        return date.replace(date, date + '-01')\n",
    "    else:\n",
    "        return date\n",
    "\n",
    "charts = charts.assign(release_date=charts.get('release_date').apply(new_date))\n",
    "charts"
   ]

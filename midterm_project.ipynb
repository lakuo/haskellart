   "source": [
    "def artist(track):\n",
    "    artists=charts[charts.get('track_name')==track].get('artist_names').unique()\n",
    "    return '; '.join(artists)\n",
    "\n",
    "repeat_titles = repeat_titles.assign(all_artists=repeat_titles.get('track_name').apply(artist))\n",
    "repeat_titles"
   ]

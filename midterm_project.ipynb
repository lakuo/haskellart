    "def calculate_rank_array(track_name):\n",
    "    df = possibly_mega[possibly_mega.get('track_name') == track_name].set_index('week_num')\n",
    "    rank_arry = np.array([])\n",
    "    for week in np.arange(1, num_weeks+1):\n",
    "        if week in df.index:\n",
    "            rank_arry = np.append(rank_arry, df.get('rank').loc[week])\n",
    "        else:\n",
    "            rank_arry = np.append(rank_arry, 201)\n",
    "    return rank_arry\n",
